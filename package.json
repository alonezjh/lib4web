{
  "name": "lib4web",
  "version": "1.0.1",
  "description": "web common lib",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "start": "npm run dev",
    "dev": "tsc -w",
    "build": "tsc",
    "buildjs": "webpack --config webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "commit": "git-cz",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "web",
    "utils",
    "validate",
    "localStorage",
    "sessionStorage"
  ],
  "author": "alonez",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alonezjh/lib4web.git"
  },
  "bugs": {
    "url": "https://github.com/alonezjh/lib4web/issues"
  },
  "homepage": "https://github.com/alonezjh/lib4web/#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jest": "^23.3.9",
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "jest-localstorage-mock": "^2.3.0",
    "prettier": "^1.15.1",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.6",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "uglifyjs-webpack-plugin": "^2.0.1"
  }
}
